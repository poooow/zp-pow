{
  "expo": {
    "name": "Zpěvníkátor",
    "slug": "zp-pow",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pow.zppow",
      "versionCode": 3,
      "permissions": [],
      "adaptiveIcon": { 
        "foregroundImage": "./assets/icon-adaptive.png",
        "backgroundColor": "#c1272d"
      }
    }
  }
}